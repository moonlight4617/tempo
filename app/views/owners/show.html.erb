<h3>マイアカウント</h3>
<% @shops.each do |shop| %>
<% if shop.del_flg != 1 %>
<%= shop.name %>
<% if shop.image? %>
<%= image_tag shop.image.url %><br>
<% end %>
<p>予約一覧</p>
<% shop.calendars.each do |cal|%>
<ul>
  <li>日付：<%= cal.rent_date %>時間：<%= cal.start_time %></li>
  <p>
    <%= link_to "予約者：#{cal.user.name}", u_show_path %><br>
    <%= link_to "メッセージを送信する", ch_show_owner_path(shop.id, user: User.find(cal.user_id).public_uid) %>
  </p>
</ul>
<% end %>
<%= link_to "店舗詳細", s_show_path(shop) %>
<%= link_to "店舗情報編集", s_edit_path(shop) %>
<%= link_to "質問に応える", ch_index_owner_path(shop) %>
<% end %>
<% end %>
<%= link_to "店舗新規登録", s_new_path %>