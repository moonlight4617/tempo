<%# usersにログイン機能がありましたので、移植しました。 %>
<h1>ログイン</h1>
<%= form_for(:user_session, url: {controller: :user_sessions, action: :create}) do |f| %>
<%= f.label :email, "Eメール" %>
<%= f.text_field :email %><br>
<%= f.label :password, "パスワード" %>
<%= f.text_field :password %><br>
<%= f.submit label="ログイン" %>

<% end %>